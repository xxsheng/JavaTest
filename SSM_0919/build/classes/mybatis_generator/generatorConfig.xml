<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

  <properties resource="db.properties"/>
  
  <classPathEntry location="${jdbc.location}"/>
  
  <context id="sqlGenerate" targetRuntime="MyBatis3">
    <commentGenerator>
        <property name="suppressDate" value="true"/>
        <property name="suppressAllComments" value="true"/>
    </commentGenerator>
  
    <jdbcConnection connectionURL="${jdbc.url}" driverClass="${jdbc.driverClass}" password="${jdbc.password}" userId="${jdbc.username}" />
    
    <javaTypeResolver>
        <property name="forceBigDecimals" value="false" />
    </javaTypeResolver>
    
    <javaModelGenerator targetPackage="cn.pojo" targetProject=".\src" >
        <property name="enableSubPackages" value="true"/>
        <property name="trimStrings" value="true" />
    </javaModelGenerator>
    
    <sqlMapGenerator targetPackage="cn.dao" targetProject=".\src" >
        <property name="enableSubPackages" value="true"/>
    </sqlMapGenerator>
    
    <javaClientGenerator targetPackage="cn.dao" targetProject=".\src" type="XMLMAPPER" >
        <property name="enableSubPackages" value="true"/>
    </javaClientGenerator>
    
    <table  tableName="items" domainObjectName="Items"
           >
            
      <columnOverride column="id" property="id" />
      <columnOverride column="name" property="nAme" />
      <columnOverride column="price" property="price" />
      <columnOverride column="detail" property="detail" />
      <columnOverride column="pic" property="pic" />
      <columnOverride column="createtime" property="createtime" />
      
    </table>
    
    <table  tableName="tb_user" domainObjectName="Tb_user"
           >
            
      <columnOverride column="id" property="Id" />
      <columnOverride column="username" property="UserName" />
      <columnOverride column="birthday" property="Birthday" />
      <columnOverride column="sex" property="Sex" />
      <columnOverride column="address" property="Address" />
      
    </table>
    
  </context>
  
  
</generatorConfiguration>